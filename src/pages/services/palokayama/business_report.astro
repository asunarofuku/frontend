---
import Activities from "../../../components/Activities.astro";
import ServicesH from "../../../components/ServicesH.astro";
import type { TocItem } from "../../../components/TableOfContents.astro";
import PalokayamaLayout from "../../../layouts/services/PalokayamaLayout.astro";
const crumbs = [
    {
        text: "ホーム",
        href: "/",
    },
    {
        text: "ぱる・おかやま",
        href: "./",
    },
    {
        text: "事業報告",
    },
];
const informations = [
    {
        date: "事業報告（H28年度）",
        title: "事業報告（H28年度）",
        imagePath: "/services/palokayama/business_report/h28.jpg",
        imageAlt: "事業報告（H28年度）",
        url: "/services/palokayama/business_report/h28.pdf",
    },
];
const tocItems: TocItem[] = [
    {
        title: "アーカイブ",
        link: "#archive",
    },
];
---

<PalokayamaLayout
    title="事業報告"
    ,
    title_en="地域活動支援センターI型 ぱる・おかやま"
    tocItems={null}
    crumbs={crumbs}
>
    <div class="container mx-auto">
        <section class="mb-24" id="archive">
            <ServicesH>アーカイブ</ServicesH>

            <Activities items={informations} />
        </section>
    </div>
</PalokayamaLayout>
